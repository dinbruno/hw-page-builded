# API URLs
NEXT_PUBLIC_API_URL=https://api.exemplo.com

# Firebase configuration
NEXT_PUBLIC_FIREBASE_API_KEY=your-api-key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your-auth-domain
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your-storage-bucket
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
NEXT_PUBLIC_FIREBASE_APP_ID=your-app-id

# Vercel API para builds específicos por tenant
VERCEL_API_TOKEN=seu-token-da-api-vercel
VERCEL_TEAM_ID=id-do-seu-time-vercel
VERCEL_PROJECT_ID=id-do-seu-projeto-vercel

# Multi-tenant configuration
# Definir como 'true' para builds específicos de tenant
NEXT_PUBLIC_IS_TENANT_BUILD=false

# IDs padrão para development
NEXT_PUBLIC_TENANT_ID=tenant-id-padrao
NEXT_PUBLIC_WORKSPACE_ID=workspace-id-padrao

# Ambiente
NEXT_PUBLIC_NODE_ENV=development 